/*CSS principal do site da Cesta Básica*/
/*Resolução 1280x 1024*/

html {
	min-height: 100%;
}

body
{
	color: #333333;
	font: normal 62.5% Tahoma,sans-serif;
	background-color: #E4E8ED;
	background-position: left bottom;
	margin-left:10px;
}

p,code,ul 
{
	padding-bottom: 1.2em;
	color:#00014e;
}

/*Listas sem marcadores*/
li 
{
	list-style: none;
}

.clearer
{
	clear: both;
}

.left 
{
	float: left;
}

.right 
{
	float: right;
}

.caixa_principal 
{
	background-color: #FFF;
	font-size: 1.2em;
	padding: 20px;
	width: 922px;
	-moz-border-radius-topleft: 2em;
	-moz-border-radius-topright: 2em; 
	-moz-border-radius-bottomleft: 2em;
	-moz-border-radius-bottomright: 2em;
	display:block;
	border-right-width: 10px;
	border-left-width: 10px;
	border-right-style: solid;
	border-left-style: solid;
	border-right-color: #B5C0CE;
	border-left-color: #B5C0CE;
	border-top-width: 10px;
	border-bottom-width: 10px;
	border-top-style: solid;
	border-bottom-style: solid;
	border-top-color: #B5C0CE;
	border-bottom-color: #B5C0CE;
	margin-top: 0;
	margin-right: auto;
	margin-bottom: 0;
	margin-left: auto;
}

/* topo do site */
.topo 
{
	padding-top:20px;
}

.cabecalho 
{
	background-color: #FFFFFF;
	background-image:url(../images/titulo.png);
	font-size: 1em;
	height: 100px;
	margin: 0 auto;
	padding: 10px 3px 5px;
	width: 912px;
}

/* menu_superior */
.menu_superior {
	background: #D9E1E5 url(../images/menu_superior.jpg);
	border: 1px solid #ffffff;
	height: 35px;
}
.menu_superior a 
{
	background: transparent url(../images/menu_superior.jpg) left top repeat-x;
	border-right: 1px solid #D8EBD8;
	color: #1C455E;
	display: block;
	float: left;
	font: bold 0.95em sans-serif;
	line-height: 35px;
	padding: 0 18px;
	text-decoration: none;
}
.menu_superior a:hover 
{
	background: transparent url(../images/menu_superior.jpg) left bottom; 
	color: #1C455E;
}

/* menu_lateral */
.menu_lateral 
{
	
	float: left;
	width: 180px;
	margin-top:1px;
	
}


.menu_lateral h2
{
	color: #1C455E;
	font-size: 1em;
	line-height: 30px;
	margin: 0;
	padding-left: 12px;
}
.menu_lateral ul
{
	padding: 0;
	margin: 10px 0px 0px 0px; 
	border: 1px solid #B5C0CE;
	list-style-position: outside;
	list-style-type:circle;
}

.menu_lateral li
{
	border-bottom: 1px solid #B5C0CE;
	color: #1C455E;
}

.menu_lateral li a
{
	font-size: 1.1em;
	color: #1C455E;
	display: block;
	padding: 8px 0 8px 10%;
	text-decoration: none;
	width: 90%;
}
.menu_lateral li a:hover 
{	
	font-size: 1.2em;
	background-color: #EBF0F3;
	color: #2E7198;	
}

/* conteudo da página em uso */
.conteudo_pagina
{
	float: right;
	margin-top:8px;
	margin-bottom:10px;
	padding: 6px 40px;
	width: 642px;
}
.conteudo_pagina .descr
{
	color: #1C455E;
	font-size: 0.9em;
	margin-bottom: 6px;
}

.conteudo_pagina li
{
	margin-left: 18px;
}

.conteudo_pagina p
{
color:#00014e;
}

.conteudo_pagina_esquerda
{
	float: left;
	margin-top:0px;
	margin-bottom:10px;
	padding: 0px 0px;
	width: 318px;
	color:#00014e;
}

.conteudo_pagina_direita
{
	float: right;
	margin-top:0px;
	margin-bottom:10px;
	padding: 0px 0px;
	width: 500px;
	color:#00014e;
}

.conteudo_sem_menu
{
	float: right;
	margin-top:8px;
	margin-bottom:10px;
	padding: 0 220px 6px 0;
	width: 642px;
}

/* rodape */
.rodape
