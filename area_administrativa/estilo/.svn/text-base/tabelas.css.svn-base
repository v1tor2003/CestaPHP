table {
	font-family:Arial, Helvetica, sans-serif;
	font-style:normal;
	font-size:13px;
	border-collapse: separate;
	border: 2px solid #999999;
	color: #1C455E;	
}

th, td {padding: 0.1em 0.6em;}

#Mcaption{
    font-family:Arial, Helvetica, sans-serif;font-style:normal; font-size:18px;
    font-weight: bold;
    margin: 1em 0 0 0;
	color: #1C455E;
	height:40px;
	vertical-align:bottom;
}

col {border-right: 1px solid #999999;}

.tdboder{ 
	border-right:1px solid #999999;
	border-left:none;
}

.tdboderCenter{ 
	border-right:1px solid #999999;
	border-left:none;
	text-align:center;
	width: 80px;
}

thead {
	color: #1C455E;
	font-size:100%;
	/*background-color: #B5C0CE;*/
	background-color: #B5C0CE;
}

th {font-weight: bold; text-align: center;border-bottom:1px solid #999999;}

.par {
	background-color: #DFE3EE;
}

.row_destaque {
	background-color: #FF9966;
}

.tdboderCod
{
	border-right:1px solid #999999;
	border-left:none;
	text-align:center;
	width: 10px;
}

.linha_resultados
{
	background-color: #CCCCCC;
	color:#FFFFFF;
}

.tabela_resultados
{
	font-family:Arial, Helvetica, sans-serif;
	font-style:normal;
	font-size:13px;
	border-collapse: separate;
	border: 2px solid #999999;
	color: #1C455E;
	margin: 20px 0 0 0;
}

.tabela_resultados caption
{
	font-family: Arial, Helvetica, sans-serif;
	font-size: 14px;
	color: #1C455E;
	text-align: left;	
	margin: 0 0 0 0;
}

.legenda_tabela
{
	color: #1C455E;
	padding: 0px 0px 10px 0px;
}

